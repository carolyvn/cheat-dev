<section class="container">
  <h3 class="gallery-name">{{topic.name}}</h3>
  <h4>Opening Hour: {{format_time topic.starting_date}}</h4>
  <h4>Closing Hour: {{format_time topic.ending_date}}</h4>

  <section class="flex-row justify-center">
    {{#each topic.subjects as |subject|}}
    {{> subject-details}}
    {{/each}}
  </section>
</section>
